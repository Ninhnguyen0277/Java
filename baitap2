package baitap;

import java.util.Scanner;

public class baitap2 {
    public static void main(String[] args){
    	Scanner sc = new Scanner(System.in);
    	System.out.print("nhap n:");
    	int n = sc.nextInt();
    	if (n%2==0){
    		System.out.print("la so chan");
    	}else {
    		System.out.print("la so le");
    	}
    sc.close();
    }
}
